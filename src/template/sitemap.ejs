<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc><%= config.site %><%= config.public_path %></loc>
  </url>
  <% articleList.forEach(({ id }) => { %>
    <url>
      <loc><%= config.site %><%= config.public_path %>/<%= id %></loc>
    </url>
  <% }) %>
</urlset>
